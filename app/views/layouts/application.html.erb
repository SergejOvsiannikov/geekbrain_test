<!DOCTYPE html>
<html>
  <head>
    <title>GeekbrainsTest</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type, msg| %>
      <div>
        <%= type %>: <%= msg %>
      </div>
    <% end %>

    <%= yield %>
  </body>
</html>
